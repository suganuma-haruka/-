package jp.co.iccom.suganuma_haruka.calculate_sales;


public class BlanchList {
	int id;          //  支店コード
	String blanchName;    // 支店名

public class BlanchListManager {
	public static void main(String[] args) {
		BlanchList[] list = new BlanchList[100];
		list[0] = new BlanchList();
		list[0].id = 001;
		list[0].blanchName = "札幌支店";
		
	}
}
